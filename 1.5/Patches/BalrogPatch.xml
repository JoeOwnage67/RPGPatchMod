<?xml version="1.0" encoding="utf-8"?>

<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Races Expanded - Custom Icons</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Big and Small - Framework</li>
					</mods>
					<match Class="PatchOperationSequence">
						<operations>
							<li Class="PatchOperationConditional">
								<xpath>Defs/XenotypeDef[defName="MSSRPG_Balrog"]/genes</xpath>	
								<match Class="PatchOperationReplace">
									<xpath>Defs/XenotypeDef[defName="MSSRPG_Balrog"]/genes</xpath>
					    			<value>
										<genes>
									      <li MayRequire="RedMattis.BigSmall.Core">BS_HeatGenerator</li>
									      <li MayRequire="Ludeon.RimWorld.Biotech">Deathless</li>
									      <li MayRequire="RedMattis.BigSmall.Core">BS_HexaHorns_Skin</li>
									      <li MayRequire="RedMattis.BigSmall.Core">JotunFrame</li>
									      <li MayRequire="sarg.alphagenes">AG_GreaterFireSpew</li>
									      <li MayRequire="sarg.alphagenes">AG_VolcanicAshMaker</li>
									      <li MayRequire="Ludeon.RimWorld.Biotech">Superclotting</li>
									      <li MayRequire="vanillaracesexpanded.archon">VRE_Maniac</li>
									      <li MayRequire="RedMattis.BigSmall.Core">MaxTemp_HugeNeed</li>
									      <li MayRequire="sarg.alphagenes">AG_ColdSlowness</li>
									      <li MayRequire="vanillaracesexpanded.archon">VRE_Aggression_ExtremelyAggressive</li>
									      <li MayRequire="Ludeon.RimWorld.Biotech">MeleeDamage_Strong</li>
									      <li MayRequire="vanillaracesexpanded.archon">VRE_SlowMeleeHitter</li>
									      <li MayRequire="RedMattis.BigSmall.Core">BS_DemonTail</li>
									      <li MayRequire="sarg.alphagenes">AG_Beauty_VeryVeryUgly</li>
									      <li MayRequire="RedMattis.BigSmall.Core">BS_StonemaskHead</li>
									      <li MayRequire="sarg.alphagenes">AG_Digitigrade</li>
									      <li MayRequire="Ludeon.RimWorld.Biotech">Body_Hulk</li>
									      <li MayRequire="Ludeon.RimWorld.Biotech">Skin_DeepRed</li>
									      <li MayRequire="sarg.alphagenes">AlphaGenes_ExoticOrganism</li>
									      <li MayRequire="RedMattis.BigSmall.Core">Undergrounder</li>
									      <li MayRequire="RedMattis.BigSmall.Core">BS_Talons</li>
									      <li MayRequire="RedMattis.BigSmall.Core">BS_AncientDemonWings</li>
									      <li MayRequire="RedMattis.BigSmall.Core">BS_SatanHead</li>
									      <li MayRequire="Ludeon.RimWorld.Biotech">AptitudeRemarkable_Melee</li>
									      <li MayRequire="void.charactereditor">SZBodySize_30</li>
									      <li MayRequire="nals.facialanimation">BS_FacialAnimDisabled</li>
									    </genes>
					    			</value>
								</match>
							</li>
						</operations>
					</match>
				</li>		
			</operations>
		</match>
		<nomatch Class="PatchOperationSequence">
			<operations>
		
			<!-- remove MSSRPG_Balrog -->
				<li Class="PatchOperationConditional">
	   				<xpath>Defs/XenotypeDef[defName="MSSRPG_Balrog"]</xpath>
					<match Class="PatchOperationRemove">
						<xpath>Defs/XenotypeDef[defName="MSSRPG_Balrog"]</xpath>
					</match>
				</li>	
				<li Class="PatchOperationConditional">
	   				<xpath>Defs/PawnKindDef[defName="MSSRPG_UnderDweller"]/xenotypeSet/xenotypeChances/MSSRPG_Balrog</xpath>
					<match Class="PatchOperationRemove">
						<xpath>Defs/PawnKindDef[defName="MSSRPG_UnderDweller"]/xenotypeSet/xenotypeChances/MSSRPG_Balrog</xpath>
					</match>
				</li>	
		
				<!-- remove MSSRPG_Adult_Mage_Pool -->
				<li Class="PatchOperationConditional">
	   				<xpath>Defs/XenotypeDef[defName="MSSRPG_Adult_Mage_Pool"]</xpath>
					<match Class="PatchOperationRemove">
						<xpath>Defs/XenotypeDef[defName="MSSRPG_Adult_Mage_Pool"]</xpath>
					</match>
				</li>
		
				<!-- remove MSSRPG_Baby_Mage_Pool -->
				<li Class="PatchOperationConditional">
	   				<xpath>Defs/XenotypeDef[defName="MSSRPG_Baby_Mage_Pool"]</xpath>
					<match Class="PatchOperationRemove">
						<xpath>Defs/XenotypeDef[defName="MSSRPG_Baby_Mage_Pool"]</xpath>
					</match>
				</li>
		
				<!-- remove MSSRPG_Druid -->
				<li Class="PatchOperationConditional">
	  					<xpath>Defs/XenotypeDef[defName="MSSRPG_Druid"]</xpath>
					<match Class="PatchOperationRemove">
						<xpath>Defs/XenotypeDef[defName="MSSRPG_Druid"]</xpath>
					</match>
				</li>
		
				<!-- remove MSSRPG_Eltexian -->
				<li Class="PatchOperationConditional">
					<xpath>Defs/XenotypeDef[defName="MSSRPG_Eltexian"]</xpath>
					<match Class="PatchOperationRemove">
						<xpath>Defs/XenotypeDef[defName="MSSRPG_Eltexian"]</xpath>
					</match>
				</li>
		
				<!-- remove MSSRPG_Mage -->
				<li Class="PatchOperationConditional">
	   				<xpath>Defs/XenotypeDef[defName="MSSRPG_Mage"]</xpath>
					<match Class="PatchOperationRemove">
						<xpath>Defs/XenotypeDef[defName="MSSRPG_Mage"]</xpath>
					</match>
				</li>
		
				<!-- remove MSSRPG_Magister -->
				<li Class="PatchOperationConditional">
	   				<xpath>Defs/XenotypeDef[defName="MSSRPG_Magister"]</xpath>
					<match Class="PatchOperationRemove">
						<xpath>Defs/XenotypeDef[defName="MSSRPG_Magister"]</xpath>
					</match>
				</li>
		
				<!-- remove MSSRPG_Nullkind -->
				<li Class="PatchOperationConditional">
	   				<xpath>Defs/XenotypeDef[defName="MSSRPG_Nullkind"]</xpath>
					<match Class="PatchOperationRemove">
						<xpath>Defs/XenotypeDef[defName="MSSRPG_Nullkind"]</xpath>
					</match>
				</li>
		
				<!-- remove MSSRPG_Warlock -->
				<li Class="PatchOperationConditional">
	   				<xpath>Defs/XenotypeDef[defName="MSSRPG_Warlock"]</xpath>
					<match Class="PatchOperationRemove">
						<xpath>Defs/XenotypeDef[defName="MSSRPG_Warlock"]</xpath>
					</match>
				</li>
			</operations>
		</nomatch>
	</Operation>
</Patch>
